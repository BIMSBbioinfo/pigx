locations:
  reads-folder: /path/to/reads/
  output-folder: /path/to/output/
  genome-fasta: /path/to/genome.fasta
  gtf-file: /path/to/sample.gtf
tools:
  fastqc:
    executable: @FASTQC@
  multiqc:
    executable: @MULTIQC@
  star:
    executable: @STAR@
    n-threads: 8
  trim-galore:
    executable: @TRIMGALORE@
    args: "--paired"
  bamCoverage:
    executable: @BAMCOVERAGE@
  samtools:
    executable: @SAMTOOLS@
  htseq-count:
    executable: @HTSEQ_COUNT@
  R:
    Rscript: @RSCRIPT@
    executable: @R@
