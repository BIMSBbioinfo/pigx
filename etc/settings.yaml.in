locations:
  input-dir: in/
  output-dir: out/
  genome-dir: genome/
  index-dir:
  annotation:
      gff-file: 

general:
  assembly: 

sample_sheet:

general:
  assembly: 
  params:
    extend: 
    scale_bw:
    bowtie2:
        k:
    idr:
        idr-threshold: 
    macs2:
        g:
        keep-dup:
        q:
    extract_signal:
        expand_peak:
        bin_num:



settings:


execution:
  submit-to-cluster: no
  jobs: 6
  nice: 19
  cluster:
    missing-file-timeout: 120
    memory: 8G
    stack: 128M
    queue: all
    contact-email: none
  rules:
    __default__:
      threads: 1
      memory: 8G
    bowtie2:
      threads: 1
      memory: 16G
    bowtie2_build:
      threads: 1
      memory: 16G

tools:
    fastqc: 
        executable: @FASTQC@
        args: ""
    bamToBed: 
        executable: @BAMTOBED@
        args: ""
    bowtie2: 
        executable: @BOWTIE2@
        args: ""
        help: '-h'
        remove:
             - S
             - x
             - 1
             - 2
             - U
    bowtie2-build: 
        executable: @BOWTIE2_BUILD@
        args: ""
    bowtie2-inspect: 
        executable: @BOWTIE2_INSPECT@
        args: ""
    idr: 
        executable: @IDR@
        args: ""
        help: '-h'
        remove:
            - samples
            - input-file-type
            - rank
            - output-file
            - l
            - plot
    macs2: 
        executable: @MACS2@
        args: ""
        help: 'callpeak -h'
        remove:
            - t
            - c
            - outdir
            - n
    perl: 
        executable: @PERL@
        args: ""
    Rscript: 
        executable: @RSCRIPT@
        args: ""
    samtools: 
        executable: @SAMTOOLS@
        args: ""
    python: 
        executable: @PYTHON@
        args: ""
    bedToBigBed:
        executable: @BEDTOBIGBED@
        args: ""

